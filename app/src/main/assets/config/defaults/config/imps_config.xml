<presrule_uri>/xcap-root/pres-rules/users/%s/pres-rules.xml</presrule_uri>

<profile_uri>/xcap-root/org.openmobilealliance.user-profile/users/%s/profile.xml</profile_uri>

<directory_uri>/xcap-root/directory/users/%s/directory.xml</directory_uri>

